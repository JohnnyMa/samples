{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "es6": true
    },
    "extends": ["eslint:recommended"],
    "globals": {
        "console": true,
        "window": true,
        "document": true
    },
    "rules": {
        "eol-last": "off",
        "no-console": "off",
        "comma-dangle": [2, "never"],
        "no-new": "off"
    }
}